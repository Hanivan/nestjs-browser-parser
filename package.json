{
  "name": "nestjs-js-parser",
  "version": "0.0.1",
  "description": "A powerful NestJS module for parsing HTML content with JavaScript support using Playwright Core",
  "author": "Your Name <your.email@example.com>",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nestjs",
    "html",
    "parser",
    "playwright",
    "javascript",
    "scraping",
    "web-scraping",
    "automation",
    "browser",
    "cdp",
    "css-selectors"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepublishOnly": "npm run build",
    "demo": "echo \"Available demos:\n  npm run demo:basic - Basic HTML parsing with JavaScript\n  npm run demo:browser - Browser configuration examples\n  npm run demo:screenshot - Screenshot and PDF generation\n  npm run demo:evaluate - JavaScript evaluation on pages\n  npm run demo:structured - Structured data extraction\n  npm run demo:typing - Enhanced TypeScript typing examples\n  npm run demo:amazon - Real Amazon search with browser interaction\n  npm run demo:all - Run all demos\n\nVerbose mode (shows detailed errors):\n  npm run demo:basic:verbose\n  npm run demo:browser:verbose\n  npm run demo:screenshot:verbose\n  npm run demo:evaluate:verbose\n  npm run demo:structured:verbose\n  npm run demo:typing:verbose\n  npm run demo:amazon:verbose\"",
    "demo:basic": "ts-node src/examples/basic-parsing.ts",
    "demo:browser": "ts-node src/examples/browser-config.ts",
    "demo:screenshot": "ts-node src/examples/screenshot-pdf.ts",
    "demo:evaluate": "ts-node src/examples/javascript-evaluation.ts",
    "demo:structured": "ts-node src/examples/structured-extraction.ts",
    "demo:typing": "ts-node src/examples/enhanced-typing.ts",
    "demo:amazon": "ts-node src/examples/amazon-real-search.ts",
    "demo:basic:verbose": "ts-node src/examples/basic-parsing.ts --verbose",
    "demo:browser:verbose": "ts-node src/examples/browser-config.ts --verbose",
    "demo:screenshot:verbose": "ts-node src/examples/screenshot-pdf.ts --verbose",
    "demo:evaluate:verbose": "ts-node src/examples/javascript-evaluation.ts --verbose",
    "demo:structured:verbose": "ts-node src/examples/structured-extraction.ts --verbose",
    "demo:typing:verbose": "ts-node src/examples/enhanced-typing.ts --verbose",
    "demo:amazon:verbose": "ts-node src/examples/amazon-real-search.ts --verbose",
    "demo:all": "npm run demo:basic && echo '\n\n' && npm run demo:browser && echo '\n\n' && npm run demo:screenshot && echo '\n\n' && npm run demo:evaluate && echo '\n\n' && npm run demo:structured && echo '\n\n' && npm run demo:typing && echo '\n\n' && npm run demo:amazon"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@types/cheerio": "^0.22.35",
    "cheerio": "^1.1.0",
    "playwright-core": "^1.53.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
